<%= render 'shared/header', title: "Register" %>

<%= form_with(model: @user, local: true) do |f| %>
  <%= render 'shared/errors', obj: @user %>
  <div>
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <%= f.submit "Register" %>
<% end %>
